{
    "env": {
        "browser": true,
        "cypress/globals": true
    },
    "extends": [
        "@antfu",
        "plugin:cypress/recommended",
        "plugin:chai-friendly/recommended"
    ],
    "plugins": [
        "cypress"
    ],
    "rules": {
        "indent": ["error", 2],
        "max-len": [
            "error", {
                "code": 100
            }
        ]
    }
}